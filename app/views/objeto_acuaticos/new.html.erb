<%# <h1>ObjetoAcuaticos#new</h1>
<p>Find me in app/views/objeto_acuaticos/new.html.erb</p>
 %>

<div class="container">
  <h1>Create new post</h1>

  <div class="bg-white rounded p-3">
      <%= simple_form_for(@objeto_acuatico) do |f| %>
        <% if @objeto_acuatico.errors.any? %>
          <div class="errors-container">
            <ul>
              <% @objeto_acuatico.errors.full_messages.each do |message| %>
                <li><%= message %></li>
              <% end %>
            </ul>
          </div>
        <% end %>
        <%= f.input :photo, as: :file %>
        <%= f.input :title %>
        <%= f.input :description %>
        <%= f.input :location %>
        <%= f.input :price %>
        <%= f.input :capacity %>
        <%= f.submit  class:'btn btn-flat'%>
      <% end %>
  </div>

  <%= link_to 'Back', root_path %>

</div>
